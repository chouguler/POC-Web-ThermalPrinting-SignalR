<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<appSettings>
		<add key="appBaseURL" value="http://localhost:6789/" />
		<!--Scanner settings for serial communication-->
		<add key="ScannerCOMPort" value="0001" />
		<add key="ScannerBaudRate" value="009600" />
		<add key="ScannerDataBits" value="8" />
		<add key="ScannerStopBits" value="1" />
		<add key="ScannerParity" value="N" />
		<!--Printer settings for serial communication-->		
		<add key="SerialPrinterCOMPort" value="0002" />
		<add key="SerialPrinterBaudRate" value="009600" />
		<add key="SerialPrinterDataBits" value="8" />
		<add key="SerialPrinterStopBits" value="1" />
		<add key="SerialPrinterParity" value="N" />
		<add key="SerialPrinterType" value="ZPL" />
		<add key="SerialPrinterSupplyPosition" value="-50" />
		<!--Printer settings for USB communication-->
		<add key="USBPrinterName" value="Zebra" />
		<add key="USBPrinterType" value="ZPL" />		
		<add key="USBPrinterSupplyPosition" value="-50" />
	</appSettings>
